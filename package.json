{
  "name": "mcp-nft-migration",
  "version": "1.0.0",
  "description": "MCP Server for NFT IPFS to Filecoin Migration using ERC-8004 and Synapse SDK",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "mcp-nft-migration": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x build/index.js && chmod +x build/index-daemon.js",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "dev": "tsc && node build/index.js",
    "daemon": "node build/index-daemon.js",
    "demo": "node examples/demo.js",
    "setup": "node lib/scripts/setup-via-sdk.js",
    "check": "node lib/scripts/pre-upload-check.js"
  },
  "keywords": [
    "mcp",
    "nft",
    "filecoin",
    "ipfs",
    "migration",
    "claude",
    "erc-8004",
    "synapse"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@filoz/synapse-sdk": "^0.35.3",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "axios": "^1.6.2",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  }
}
